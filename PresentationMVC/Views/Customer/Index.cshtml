@model List<CustomerViewModel>

<h2>Kunder</h2>

<table class="table table-striped table-bordered table-hover mt-4">
    <thead class="table-dark">
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Namn</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var customer in Model)
        {
            <tr>
                <td>@customer.Id</td>
                <td>@customer.Name</td>
            </tr>
        }
    </tbody>
</table>
